{
  "name": "RTL_433 to MQTT Bridge with Interlogix auto-discovery",
  "version": "0.1.3",
  "slug": "rtl4332mqtt-interlogix-autodiscovery",
  "description": "433MHz RF to MQTT Bridge based on RTL_SDR/RTL_433 for RTL2832U based DVB-T USB tuners",
  "url": "https://github.com/yannicklarrivee/homeassistant-addons-master",
  "startup": "before",
  "boot": "auto",
  "map": ["config:rw", "ssl"],
  "devices": ["/dev/bus/usb:/dev/bus/usb:rwm"],
  "host_network": "False",
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "options":
  {
    "mqtt_host": "homeassistant",
    "mqtt_user": "",
    "mqtt_password": "",
    "mqtt_topic": "rtl_433",
    "protocol": 100,
    "frequency": 319500000,
    "gain": 60,
    "frequency_offset": 350,
    "sensors": []
  },
  "schema":
  {
    "mqtt_host": "str",
    "mqtt_user": "str",
    "mqtt_password": "str",
    "mqtt_topic": "str",
    "protocol": "int",
    "frequency": "int",
    "gain": "int",
    "frequency_offset": "int",
    "sensors": [{"id":"str", "name":"str"}]
   }
}
