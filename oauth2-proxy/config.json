{
  "name": "oauth2-proxy",
  "version": "0.4.0",
  "slug": "oauth2-proxy",
  "description": "Start an oauth2-proxy for external authentication.",
  "url": "https://github.com/yannicklarrivee/homeassistant-addons-master/tree/master/oauth2-proxy",
  "startup": "application",
  "boot": "auto",
  "map": ["config","share:rw"],
  "host_network": "true",
  "ports": {
    "4180/tcp": 4180
  },
  "arch": [
    "aarch64",
    "amd64",
    "armv7",
    "i386"
  ],
  "options":
  {
    "provider": "google",
    "clientid": "xxxxxxxxx",
    "clientsecret": "xxxxxxx",
    "emaildomain": "*",
    "cookiesecret": "xxxxxxx",
    "authenticatedemailsfile": "authenticated_emails.conf"

  },
  "schema":
  {
    "provider": "str",
    "clientid": "str",
    "clientsecret": "str",
    "emaildomain": "str",
    "cookiesecret": "str",
    "callbackurl": "str",
    "authenticatedemailsfile": "str"
   }
}
